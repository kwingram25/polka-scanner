    "@babel/core": "^7.17.5",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^7.3.0",
    "cypress": "^10.4.0",
    "eslint": "^8.9.0",
