    "@emotion/styled": "^11.8.1",
    "@polkadot/api": "^9.1.1",
    "@polkadot/react-identicon": "^2.9.4",
    "@polkadot/types": "^9.1.1",
    "@polkadot/util": "^10.1.3",
    "@tanstack/react-table": "^8.5.11",
    "chakra-react-select": "^4.1.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "focus-visible": "^5.2.0",
    "formik": "^2.2.9",
    "framer-motion": "^6.2.8",
    "react": "^17.0.2",
    "react-device-detect": "^2.2.2",
    "react-dom": "^17.0.2",
