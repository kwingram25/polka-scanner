  "scripts": {
    "dev": "vite",
    "build": "yarn run client:build && yarn run server:build",
    "start": "node dist/server.js",
    "client:build": "tsc && vite build",
    "server:build": "tsc --project tsconfig.server.json",
    "serve": "vite preview",
    "lint": "eslint src --ext .tsx --ext .ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --config ./.prettierrc -w 'src/**/*.{tsx,ts}' && git update-index --again",
    "test": "concurrently \"yarn dev\" \"yarn cypress run\""
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.5",
