    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-react-jsx": "^1.1.2"
  }
